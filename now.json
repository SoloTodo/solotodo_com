{
  "version": 2,
  "name": "solotodo_com",
  "alias": [
    "www.solotodo.com"
  ],
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "routes": [
    {
      "src": "/",
      "dest": "/",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/tos",
      "dest": "/tos",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/reset/(?<uid>[^/]+?)/(?<token>[^/]+?)/?",
      "dest": "/reset?uid=$uid&token=$token",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/products/(?<id>[^/]+?)-(?<slug>[^/]*)/?",
      "dest": "/products/view?id=$id&slug=$slug",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/products/(?<id>[^/]+?)/?",
      "dest": "/products/view?id=$id",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/products/(?<id>[^/]+?)/ratings/?",
      "dest": "/products/ratings?id=$id",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/products/(?<id>[^/]+?)/ratings/new/?",
      "dest": "/products/new_rating?id=$id",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/stores/(?<id>[^/]+?)/ratings/?",
      "dest": "/store_ratings?id=$id",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/budgets/compatibility/?",
      "dest": "/budgets/compatibility",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/budgets/(?<id>[^/]+?)/?",
      "dest": "/budgets/view?id=$id",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/budgets/(?<id>[^/]+?)/edit/?",
      "dest": "/budgets/edit?id=$id",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/notebook_processors/?",
      "dest": "/notebook_processors",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/notebook_video_cards/?",
      "dest": "/notebook_video_cards",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/cyber_check/?",
      "dest": "/cyber_check",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/video_card_gpus/(?<id>[^/]+?)/?",
      "dest": "/video_card_gpus?id=$id",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/search/?",
      "dest": "/search?search=$keyword",
      "headers": { "cache-control": "no-cache" }
    },
    {
      "src": "/ads.txt",
      "status": 301,
      "headers": {
        "Location": "/static/ads.txt"
      }
    },
    {
      "src": "/robots.txt",
      "status": 301,
      "headers": {
        "Location": "/static/robots.txt"
      }
    },
    {
      "src": "/sitemap.xml",
      "status": 301,
      "headers": {
        "Location": "https://solotodo-core.s3.amazonaws.com/sitemaps/sitemap.xml"
      }
    },
    {
      "src": "/(?<categoryslug>[^/]+)/?",
      "dest": "/browse?category_slug=$categoryslug",
      "headers": { "cache-control": "no-cache" }
    }
  ]
}
